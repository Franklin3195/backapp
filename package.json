{
    "name": "appyudame-nodejs",
    "version": "1.0.3",
    "description": "Backend project for AppYudame",
    "author": "AppYudame",
    "private": true,
    "license": "MIT",
    "scripts": {
        "start": "nest start",
        "build": "nest build",
        "postbuild": "copyfiles --up 1 src/**/*.json src/**/*.hbs dist",
        "postbuild:prod": "copyfiles --up 1 src/**/*.json src/**/*.hbs dist",
        "build:prod": "tsc -p tsconfig.build.json",
        "start:hmr": "node dist/main.hmr.ts",
        "start:dev": "ts-node src/main.ts",
        "dev": "ts-node-dev src/main.ts",
        "dev:test": "cross-env PARAMS_ENV=local_test ts-node src/main.ts",
        "start:prod": "node dist/main.js",
        "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
        "migrations:generate": "yarn run typeorm migration:generate -d ormconfig.ts",
        "migrations:create": "yarn run typeorm migration:create",
        "migrations:run": "yarn run typeorm migration:run -- -d ormconfig",
        "seed:run": "ts-node ./node_modules/typeorm-extension/bin/cli.cjs seed:run -d ormconfig",
        "new": "hygen new module",
        "migrations:revert": "yarn run typeorm migration:revert -d ormconfig",
        "schema:drop": "yarn run typeorm schema:drop -d ormconfig",
        "watch:dev": "ts-node-dev src/main.ts",
        "debug:dev": "cross-env TS_NODE_CACHE=false ts-node-dev --inspect --ignore '/^src/.*\\.spec\\.ts$/' src/main.ts",
        "webpack": "webpack --config webpack.config.js --progress",
        "lint": "eslint . --ext .ts",
        "lint:fix": "eslint --fix . --ext .ts",
        "test": "cross-env PARAMS_ENV=local_test jest",
        "test:watch": "cross-env PARAMS_ENV=local_test jest --watch",
        "test:cov": "cross-env PARAMS_ENV=local_test jest --coverage",
        "test:debug": "cross-env PARAMS_ENV=local_test node --inspect-brk -r ts-node/register node_modules/.bin/jest --runInBand",
        "copy:lib": "cpx 'src/lib/**/{files,workspace}/**/*.*' dist/lib && cpx 'src/lib/**/{files,workspace}/**/.!(gitignore)' dist/lib",
        "test:e2e": "cross-env PARAMS_ENV=local_test jest --config ./test/jest-e2e.json",
        "docs:dev": "vitepress dev docs",
        "docs:build": "vitepress build docs",
        "docs:preview": "vitepress preview docs",
        "docs:deploy": "yarn docs:build && gh-pages -d .vitepress/dist",
        "prepare": "husky install",
        "prerelease": "yarn docs:deploy",
        "release": "release-it"
    },
    "dependencies": {
        "@aws-sdk/client-cognito-identity-provider": "^3.418.0",
        "@aws-sdk/client-s3": "^3.369.0",
        "@aws-sdk/client-ses": "^3.418.0",
        "@aws-sdk/client-sns": "^3.445.0",
        "@aws-sdk/s3-request-presigner": "^3.445.0",
        "@nestjs/common": "^10.3.3",
        "@nestjs/config": "^3.0.0",
        "@nestjs/core": "^10.0.5",
        "@nestjs/cqrs": "^10.0.1",
        "@nestjs/jwt": "^10.1.0",
        "@nestjs/microservices": "^10.0.5",
        "@nestjs/passport": "^10.0.0",
        "@nestjs/platform-express": "^10.3.3",
        "@nestjs/platform-socket.io": "^10.2.7",
        "@nestjs/schedule": "^4.0.0",
        "@nestjs/swagger": "^7.1.1",
        "@nestjs/terminus": "^10.0.1",
        "@nestjs/typeorm": "^10.0.0",
        "@nestjs/websockets": "^10.2.7",
        "@types/auth0": "^3.3.10",
        "archiver": "^7.0.1",
        "auth0": "^4.2.0",
        "bcrypt": "^5.1.0",
        "cache-manager-redis-store": "^3.0.1",
        "class-transformer": "~0.5.1",
        "class-validator": "^0.14.0",
        "compression": "^1.7.4",
        "date-fns": "^3.4.0",
        "exceljs": "^4.4.0",
        "express": "^4.18.2",
        "express-ctx": "^0.1.1",
        "express-rate-limit": "^7.1.4",
        "fs-extra": "^11.2.0",
        "fs.promises": "^0.1.2",
        "handlebars": "^4.7.8",
        "helmet": "^7.0.0",
        "html-to-text": "^9.0.5",
        "iconv-lite": "^0.6.3",
        "jsonwebtoken": "^9.0.1",
        "jwks-rsa": "^3.0.1",
        "lodash": "^4.17.21",
        "mime-types": "^2.1.35",
        "moment": "^2.29.4",
        "multer": "^1.4.4-lts.1",
        "mysql2": "^3.9.2",
        "nanoid": "3.3.7",
        "nestjs-pino": "^3.5.0",
        "openai": "^4.24.1",
        "passport": "^0.6.0",
        "passport-jwt": "^4.0.1",
        "path": "^0.12.7",
        "pdfkit": "^0.14.0",
        "pg": "^8.11.3",
        "pino-http": "^8.5.1",
        "pino-pretty": "^10.2.0",
        "read-chunk": "^4.0.3",
        "reflect-metadata": "^0.1.13",
        "rxjs": "^7.8.1",
        "source-map-support": "^0.5.21",
        "sqlite3": "^5.1.6",
        "swagger-ui-express": "^5.0.0",
        "twilio": "^4.18.0",
        "typeorm": "^0.3.20",
        "typeorm-extension": "^3.0.1",
        "typeorm-transactional": "~0.4.1",
        "unzipper": "^0.10.14",
        "uuid": "^9.0.0",
        "xlsx": "^0.18.5",
        "xlsx-populate": "^1.21.0",
        "yauzl": "^3.1.1"
    },
    "devDependencies": {
        "@faker-js/faker": "^8.2.0",
        "@handfish/hygen": "^6.1.6",
        "@nestjs/cli": "^10.1.8",
        "@nestjs/schematics": "^10.0.2",
        "@nestjs/testing": "^10.0.5",
        "@types/bcrypt": "^5.0.2",
        "@types/cache-manager-redis-store": "^2.0.4",
        "@types/compression": "^1.7.2",
        "@types/express": "^4.17.21",
        "@types/jest": "^29.5.8",
        "@types/jsonwebtoken": "^9.0.1",
        "@types/lodash": "^4.14.191",
        "@types/mime-types": "^2.1.4",
        "@types/morgan": "^1.9.9",
        "@types/multer": "^1.4.11",
        "@types/node": "^20.11.25",
        "@types/passport-jwt": "^3.0.13",
        "@types/supertest": "^2.0.12",
        "@types/uuid": "^9.0.2",
        "@types/webpack-env": "^1.18.1",
        "@types/xlsx": "^0.0.36",
        "@typescript-eslint/eslint-plugin": "^6.10.0",
        "@typescript-eslint/parser": "^6.10.0",
        "clean-webpack-plugin": "^4.0.0",
        "copyfiles": "^2.4.1",
        "cpx": "^1.5.0",
        "cross-env": "^7.0.3",
        "eslint": "^8.44.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-import-resolver-typescript": "^3.5.5",
        "eslint-plugin-import": "^2.28.1",
        "eslint-plugin-import-helpers": "^1.3.1",
        "eslint-plugin-prettier": "^5.0.0",
        "eslint-plugin-simple-import-sort": "^10.0.0",
        "eslint-plugin-sonarjs": "^0.23.0",
        "eslint-plugin-unicorn": "^48.0.1",
        "gh-pages": "^6.0.0",
        "husky": "^8.0.3",
        "jest": "^29.6.1",
        "jest-mock-extended": "^3.0.5",
        "lint-staged": "~15.1.0",
        "prettier": "^3.0.0",
        "release-it": "^17.0.0",
        "supertest": "^6.3.3",
        "ts-jest": "^29.1.1",
        "ts-node": "^10.9.1",
        "ts-node-dev": "^2.0.0",
        "typescript": "^5.4.2",
        "vitepress": "^1.0.0-rc.25",
        "webpack": "^5.88.1",
        "webpack-cli": "^5.1.4",
        "webpack-node-externals": "^3.0.0"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "testRegex": ".spec.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "coverageDirectory": "../coverage",
        "testEnvironment": "node",
        "setupFilesAfterEnv": [
            "<rootDir>/jest.setup.ts"
        ]
    },
    "resolutions": {
        "multer": "1.4.4-lts.1"
    },
    "engines": {
        "node": "18.x"
    }
}
